{
    "root": true,
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "@react-native-comunity",
        "plugin:react/recommended",
        "standard"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {}
}